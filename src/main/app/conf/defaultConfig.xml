<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<configuration xmlns="http://openprovenance.org/model/opmPrinterConfig">

  <graphName>OPMGraph</graphName>


  <processes displayValue="true">
    <process value="http://process.org/add1ToAll" display="add1ToAll"/>
    <process value="http://process.org/split"     display="split"/>
    <process value="http://process.org/plus1"     display="+1"/>
    <process value="http://process.org/cons"      display="cons"/>
    <process value="http://process.org/fry"       display="fry"/>
    <process value="http://process.org/bake"      display="bake"/>
    <process value="http://process.org/badBake"   display="badBake"/>
  </processes>

  <artifacts displayValue="true"   style="bold">
    <artifact value="2 eggs"       display="two eggs"/>
  </artifacts>

  <agents displayValue="true" defaultColor="green">
  </agents>


  <edges default="filled">
    <edge type="org.openprovenance.model.Used"            style="filled"  printRole="true"/>
    <edge type="org.openprovenance.model.WasGeneratedBy"  style="filled"  printRole="true"/>
    <edge type="org.openprovenance.model.WasControlledBy" style="dotted"  printRole="true"/>
    <edge type="org.openprovenance.model.WasDerivedFrom"  style="filled"/>
  </edges>


  <accounts defaultAccount="black" defaultColor="black">
    <account name="dataflow"      color="blue"/>
  </accounts>

</configuration>  
